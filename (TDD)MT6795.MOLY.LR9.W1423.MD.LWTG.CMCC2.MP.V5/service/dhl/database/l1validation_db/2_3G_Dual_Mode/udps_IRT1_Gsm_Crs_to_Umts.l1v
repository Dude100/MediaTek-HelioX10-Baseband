DRX7  7
DRX8  8
DRX9  9

[PI_num] "Number of paging indicators per frame (Np)"
@PI18  18
PI36  36
PI72  72
PI144 144

[page_occa] "Paging occassion when IMSI mod DRX, n=0"
0~511
@0

[DRX_index] "DRX_index defined in 25.304 (IMSI/8192)"
